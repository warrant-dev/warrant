{
    "ignoredFields": [
        "createdAt",
        "processingTime"
    ],
    "tests": [
        {
            "name": "createObjectTypeReport",
            "request": {
                "method": "POST",
                "url": "/v1/object-types",
                "body": {
                    "type": "report",
                    "relations": {
                        "owner": {},
                        "editor": {
                            "inheritIf": "owner"
                        },
                        "viewer": {
                            "inheritIf": "editor"
                        },
                        "editor-viewer": {
                            "inheritIf": "allOf",
                            "rules": [
                                {
                                    "inheritIf": "editor"
                                },
                                {
                                    "inheritIf": "viewer"
                                }
                            ]
                        },
                        "non-owner": {
                            "inheritIf": "noneOf",
                            "rules": [
                                {
                                    "inheritIf": "owner"
                                }
                            ]
                        }
                    }
                }
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "type": "report",
                    "relations": {
                        "owner": {},
                        "editor": {
                            "inheritIf": "owner"
                        },
                        "viewer": {
                            "inheritIf": "editor"
                        },
                        "editor-viewer": {
                            "inheritIf": "allOf",
                            "rules": [
                                {
                                    "inheritIf": "editor"
                                },
                                {
                                    "inheritIf": "viewer"
                                }
                            ]
                        },
                        "non-owner": {
                            "inheritIf": "noneOf",
                            "rules": [
                                {
                                    "inheritIf": "owner"
                                }
                            ]
                        }
                    }
                }
            }
        },
