{
    "ignoredFields": [
        "createdAt"
    ],
    "tests": [
        {
            "name": "createUserUsera",
            "request": {
                "method": "POST",
                "url": "/v1/users",
                "body": {
                    "userId": "user-a",
                    "email": "user-a@warrant.dev"
                }
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "userId": "user-a",
                    "email": "user-a@warrant.dev"
                }
            }
        },
        {
            "name": "createRoleSeniorAccountant",
            "request": {
                "method": "POST",
                "url": "/v1/roles",
                "body": {
                    "roleId": "senior-accountant",
                    "name": "Senior Accountant",
                    "description": "Grants access to view and edit the balance sheet."
                }
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "roleId": "senior-accountant",
                    "name": "Senior Accountant",
                    "description": "Grants access to view and edit the balance sheet."
                }
            }
        },
        {
            "name": "createPermissionViewBalanceSheet",
            "request": {
                "method": "POST",
                "url": "/v1/permissions",
                "body": {
                    "permissionId": "view-balance-sheet",
                    "name": "View Balance Sheet",
                    "description": "Grants access to view the balance sheet."
                }
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "permissionId": "view-balance-sheet",
                    "name": "View Balance Sheet",
                    "description": "Grants access to view the balance sheet."
                }
            }
        },
        {
            "name": "createPermissionEditBalanceSheet",
            "request": {
                "method": "POST",
                "url": "/v1/permissions",
                "body": {
                    "permissionId": "edit-balance-sheet",
                    "name": "Edit Balance Sheet",
                    "description": "Grants access to edit the balance sheet."
                }
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "permissionId": "edit-balance-sheet",
                    "name": "Edit Balance Sheet",
                    "description": "Grants access to edit the balance sheet."
                }
            }
        },
        {
            "name": "assignPermissionViewBalanceSheetToRoleSeniorAccountant",
            "request": {
                "method": "POST",
                "url": "/v1/warrants",
                "body": {
                    "objectType": "permission",
                    "objectId": "view-balance-sheet",
                    "relation": "member",
                    "subject": {
                        "objectType": "role",
                        "objectId": "senior-accountant"
                    }
                }
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "objectType": "permission",
                    "objectId": "view-balance-sheet",
                    "relation": "member",
                    "subject": {
                        "objectType": "role",
                        "objectId": "senior-accountant"
                    }
                }
            }
        },
        {
            "name": "assignPermissionEditBalanceSheetToUserUsera",
            "request": {
                "method": "POST",
                "url": "/v1/warrants",
                "body": {
                    "objectType": "permission",
                    "objectId": "edit-balance-sheet",
                    "relation": "member",
                    "subject": {
                        "objectType": "user",
                        "objectId": "user-a"
                    }
                }
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "objectType": "permission",
                    "objectId": "edit-balance-sheet",
                    "relation": "member",
                    "subject": {
                        "objectType": "user",
                        "objectId": "user-a"
                    }
                }
            }
        },
