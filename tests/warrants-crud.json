{
    "ignoredFields": [
        "createdAt"
    ],
    "tests": [
        {
            "name": "assignPermissionViewBalanceSheetToRoleSeniorAccountant",
            "request": {
                "method": "POST",
                "url": "/v1/warrants",
                "body": {
                    "objectType": "permission",
                    "objectId": "view-balance-sheet",
                    "relation": "member",
                    "subject": {
                        "objectType": "role",
                        "objectId": "senior-accountant"
                    }
                }
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "objectType": "permission",
                    "objectId": "view-balance-sheet",
                    "relation": "member",
                    "subject": {
                        "objectType": "role",
                        "objectId": "senior-accountant"
                    }
                }
            }
        },
        {
            "name": "confirmPermissionViewBalanceSheetCreated",
            "request": {
                "method": "GET",
                "url": "/v1/permissions/view-balance-sheet"
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "permissionId": "view-balance-sheet",
                    "name": null,
                    "description": null
                }
            }
        },
        {
            "name": "confirmRoleSeniorAccountantCreated",
            "request": {
                "method": "GET",
                "url": "/v1/roles/senior-accountant"
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "roleId": "senior-accountant",
                    "name": null,
                    "description": null
                }
            }
        },
        {
            "name": "failToCreateDuplicateWarrant",
            "request": {
                "method": "POST",
                "url": "/v1/warrants",
                "body": {
                    "objectType": "permission",
                    "objectId": "view-balance-sheet",
                    "relation": "member",
                    "subject": {
                        "objectType": "role",
                        "objectId": "senior-accountant"
                    }
                }
            },
            "expectedResponse": {
                "statusCode": 400,
                "body": {
                    "code": "duplicate_record",
                    "message": "Duplicate Warrant permission:view-balance-sheet#member@role:senior-accountant, A warrant with the given objectType, objectId, relation, subject, and policy already exists",
                    "type": "Warrant",
                    "key": {
                        "objectType": "permission",
                        "objectId": "view-balance-sheet",
                        "relation": "member",
                        "subject": {
                            "objectType": "role",
                            "objectId": "senior-accountant"
                        }
                    }
                }
            }
        },
        {
            "name": "assignPermissionEditBalanceSheetToUserUsera",
            "request": {
                "method": "POST",
                "url": "/v1/warrants",
                "body": {
                    "objectType": "permission",
                    "objectId": "balance-sheet:edit",
                    "relation": "member",
                    "subject": {
                        "objectType": "user",
                        "objectId": "user-a"
                    }
                }
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "objectType": "permission",
                    "objectId": "balance-sheet:edit",
                    "relation": "member",
                    "subject": {
                        "objectType": "user",
                        "objectId": "user-a"
                    }
                }
            }
        },
        {
            "name": "confirmPermissionEditBalanceSheetCreated",
            "request": {
                "method": "GET",
                "url": "/v1/permissions/balance-sheet:edit"
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "permissionId": "balance-sheet:edit",
                    "name": null,
                    "description": null
                }
            }
        },
        {
            "name": "confirmUserACreated",
            "request": {
                "method": "GET",
                "url": "/v1/users/user-a"
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "userId": "user-a",
                    "email": null
                }
            }
        },
        {
            "name": "assignRoleSeniorAccountantToUserWithContext",
            "request": {
                "method": "POST",
                "url": "/v1/warrants",
                "body": {
                    "objectType": "role",
                    "objectId": "senior-accountant",
                    "relation": "member",
                    "subject": {
                        "objectType": "user",
                        "objectId": "user-a"
                    },
                    "policy": "tenant == \"tenant-a\" && organization == \"org-a\""
                }
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "objectType": "role",
                    "objectId": "senior-accountant",
                    "relation": "member",
                    "subject": {
                        "objectType": "user",
                        "objectId": "user-a"
                    },
                    "policy": "tenant == \"tenant-a\" && organization == \"org-a\""
                }
            }
        },
        {
            "name": "removePermissionViewBalanceSheetFromRoleSeniorAccountant",
            "request": {
                "method": "DELETE",
                "url": "/v1/warrants",
                "body": {
                    "objectType": "permission",
                    "objectId": "view-balance-sheet",
                    "relation": "member",
                    "subject": {
                        "objectType": "role",
                        "objectId": "senior-accountant"
                    }
                }
            },
            "expectedResponse": {
                "statusCode": 200
            }
        },
        {
            "name": "deletePermissionEditBalanceSheet",
            "request": {
                "method": "DELETE",
                "url": "/v1/permissions/balance-sheet:edit",
                "body": {
                    "permissionId": "balance-sheet:edit"
                }
            },
            "expectedResponse": {
                "statusCode": 200
            }
        },
        {
            "name": "deletePermissionViewBalanceSheet",
            "request": {
                "method": "DELETE",
                "url": "/v1/permissions/view-balance-sheet"
            },
            "expectedResponse": {
                "statusCode": 200
            }
        },
        {
            "name": "deleteRoleSeniorAccountant",
            "request": {
                "method": "DELETE",
                "url": "/v1/roles/senior-accountant"
            },
            "expectedResponse": {
                "statusCode": 200
            }
        },
        {
            "name": "deleteUserUserA",
            "request": {
                "method": "DELETE",
                "url": "/v1/users/user-a"
            },
            "expectedResponse": {
                "statusCode": 200
            }
        },
        {
            "name": "confirmAllWarrantsDeleted",
            "request": {
                "method": "GET",
                "url": "/v1/warrants"
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": []
            }
        }
    ]
}
