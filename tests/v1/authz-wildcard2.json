        {
            "name": "createObjectTypeSchedule",
            "request": {
                "method": "POST",
                "url": "/v1/object-types",
                "body": {
                    "type": "schedule",
                    "relations": {
                        "editor": {
                            "inheritIf": "anyOf",
                            "rules": [
                                {
                                    "inheritIf": "manager",
                                    "ofType": "department",
                                    "withRelation": "parent"
                                },
                                {
                                    "inheritIf": "manager",
                                    "ofType": "division",
                                    "withRelation": "parent"
                                },
                                {
                                    "inheritIf": "admin",
                                    "ofType": "organization",
                                    "withRelation": "parent"
                                }
                            ]
                        },
                        "parent": {
                            "inheritIf": "parent",
                            "ofType": "department",
                            "withRelation": "parent"
                        },
                        "viewer": {
                            "inheritIf": "editor"
                        }
                    }
                }
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "type": "schedule",
                    "relations": {
                        "editor": {
                            "inheritIf": "anyOf",
                            "rules": [
                                {
                                    "inheritIf": "manager",
                                    "ofType": "department",
                                    "withRelation": "parent"
                                },
                                {
                                    "inheritIf": "manager",
                                    "ofType": "division",
                                    "withRelation": "parent"
                                },
                                {
                                    "inheritIf": "admin",
                                    "ofType": "organization",
                                    "withRelation": "parent"
                                }
                            ]
                        },
                        "parent": {
                            "inheritIf": "parent",
                            "ofType": "department",
                            "withRelation": "parent"
                        },
                        "viewer": {
                            "inheritIf": "editor"
                        }
                    }
                }
            }
        },
