    "tests": [
        {
            "name": "createObjectTypeDepartment",
            "request": {
                "method": "POST",
                "url": "/v1/object-types",
                "body": {
                    "type": "department",
                    "relations": {
                        "manager": {
                            "inheritIf": "manager",
                            "ofType": "division",
                            "withRelation": "parent"
                        },
                        "member": {
                            "inheritIf": "manager"
                        },
                        "parent": {
                            "inheritIf": "parent",
                            "ofType": "division",
                            "withRelation": "parent"
                        }
                    }
                }
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "type": "department",
                    "relations": {
                        "manager": {
                            "inheritIf": "manager",
                            "ofType": "division",
                            "withRelation": "parent"
                        },
                        "member": {
                            "inheritIf": "manager"
                        },
                        "parent": {
                            "inheritIf": "parent",
                            "ofType": "division",
                            "withRelation": "parent"
                        }
                    }
                }
            }
        },
