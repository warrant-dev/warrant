                    },
                    "policy": "transaction.amount < (0.2 * limit)"
                }
            },
            "expectedResponse": {
                "statusCode": 200,
                "body": {
                    "objectType": "bank-account",
                    "objectId": "personal-checking",
                    "relation": "withdraw",
                    "subject": {
                        "objectType": "user",
                        "objectId": "user-a"
                    },
